<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.meteor"
		name="Nintendo - Game Boy Advance (Meteor)"
		version="1.4.0.18"
		provider-name="Philippe Daouadi">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.gba" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>Game Boy Advance</platforms>
		<extensions>gba</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>false</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv3</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.meteor</source>
		<assets>
			<icon>resources/icon.png</icon>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
		<summary lang="en_GB">Nintendo - Game Boy Advance (Meteor) v1.4</summary>
		<description lang="da_DK">Meteor er en Nintendo Gameboy Advance-emulator.</description>
		<description lang="en_GB">Meteor is a Nintendo Gameboy Advance emulator.</description>
		<description lang="es_ES">Meteor es un emulador de Nintendo Gameboy Advance.</description>
		<description lang="it_IT">Meteor è un emulatore di Nintendo Gameboy Advance.</description>
		<description lang="ko_KR">Meteor는 닌텐도 게임보이 어드밴스 에뮬레이터입니다.</description>
		<description lang="zh_CN">Meteor 是任天堂 Gameboy Advance 游戏机模拟器。</description>
